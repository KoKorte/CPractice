        scanf("%d", &y);
    printf("\nSyöttämäsi lukujen x ja y summa on: %d", x+y);
    */
    
//Answer
/*
#include<stdio.h>
int main() {
int a, b, sum;
a=1;
b=2;
sum = a + b;
printf("The sum of a and b = %d", sum);
return 0;
}
*/

    //10. Write a program to find the square of a number.

    /*
    int x = 13;
