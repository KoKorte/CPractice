        scanf("%d", &x);
    if (x%2 == 0){
        printf("\nSyöttämäsi numero on parillinen!");
    }
    else{
        printf("\nSyöttämäsi numero on pariton!");
    }
    */

//Answer

/*
#include<stdio.h>
int main() {
int a;
printf("Enter any number: ");
scanf ("%d", &a);
if(a%2 == 0) {
printf("The entered number is even");
}
else {
printf("The entered number is odd");
}
return 0;
}
*/

